{
    "name": "sicaboy/laravel-newsletter",
    "description": "Manage newsletters in Laravel",
    "keywords": [
        "laravel",
        "newsletter",
        "mailchimp"
    ],
    "homepage": "https://github.com/sicaboy/laravel-newsletter",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://murze.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "drewm/mailchimp-api": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.3|^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Newsletter\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sicaboy\\Newsletter\\NewsletterServiceProvider"
            ],
            "aliases": {
                "Newsletter": "Sicaboy\\Newsletter\\NewsletterFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
